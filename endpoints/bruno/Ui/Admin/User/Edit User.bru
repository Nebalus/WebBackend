meta {
  name: Edit User
  type: http
  seq: 3
}

put {
  url: {{protocol}}://{{host}}/ui/admin/users/:userid
  body: json
  auth: bearer
}

params:path {
  userid: self
}

auth:bearer {
  token: {{auth_jwt}}
}

body:json {
  {
    "username": "testadmin",
    "email": "admin@nebalus.dev",
    "email_verified": true
  }
}

vars:pre-request {
  userid: 1
}

settings {
  encodeUrl: true
  timeout: 0
}
