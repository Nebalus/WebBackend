meta {
  name: Get Role With Permissions
  type: http
  seq: 5
}

get {
  url: {{protocol}}://{{host}}/ui/admin/role/:roleId
  body: json
  auth: apikey
}

params:path {
  roleId: 1
}

auth:apikey {
  key: Authorization
  value: {{auth_jwt}}
  placement: header
}

body:json {
  {
    "with_permissions": true
  }
}
