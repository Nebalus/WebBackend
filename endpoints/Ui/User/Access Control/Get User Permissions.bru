meta {
  name: Get User Permissions
  type: http
  seq: 1
}

get {
  url: {{protocol}}://{{host}}/ui/user/:userId/permissions
  body: none
  auth: apikey
}

params:path {
  userId: self
}

auth:apikey {
  key: Authorization
  value: {{auth_jwt}}
  placement: header
}

docs {
  This endpoint is used to show an Aggrigation from all the roles the user of all Permissions that the user has.
}
